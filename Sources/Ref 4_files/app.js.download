    switch (dir) {
      case 'next':
    
        par.page = (par.page >= par.pages) ? par.page : par.page + 1;
        break;
      case 'prev':
        par.page = (par.page <= 1) ? par.pages : par.page - 1;
        break;
      default: // 'to':
        par.page = (dir >= 1 && dir <= par.pages) ? dir : 1;
        break;
